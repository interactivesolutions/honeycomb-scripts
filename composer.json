  "require": {
    "php": ">=7.1",
    "laravel\/framework": "5.3.*",
    "nette\/reflection": "^2.4",
    "xethron\/migrations-generator": "^2.0",
    "interactivesolutions/honeycomb-core": "~1.0.0",
    "interactivesolutions/honeycomb-languages": "~1.0.0"
  },
  "require-dev": {
    "brianium/paratest": "^0.14",
    "mockery/mockery": "^0.9",
    "orchestra/testbench-browser-kit": "^3.3",
    "phpunit/phpunit": "^5.7"
  },
  "autoload": {
    "psr-4": {
      "InteractiveSolutions\\HoneycombScripts\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "scripts": {
    "test": [
      "paratest -p4 --colors"
    ]
  },
  "config": {
    "secure-http": false,
    "platform": {
      "php": "7.1"
    }
  }
}
