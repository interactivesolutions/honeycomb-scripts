{
  "name": "interactivesolutions/honeycomb-scripts",
  "description": "HoneyComb CMS scripts package",
  "keywords": ["honeycomb", "honey", "laravel", "scripts", "automation"],
  "license": "MIT",
  "repositries" : [],
  "authors": [
    {
      "name": "Jevgenij Dmitrijev",
      "email": "jd@interactivesolutions.lt"
    },
    {
      "name": "Mantas Gelčius",
      "email": "mantas.gelcius@gmail.com"
    },
    {
      "name": "Liutauras Kavaliauskas",
